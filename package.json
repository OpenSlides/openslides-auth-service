{
    "name": "openslides-auth-service",
    "version": "1.0.0",
    "description": "Service for OpenSlides which ...",
    "main": "build/server.js",
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "preinstall": "npm run uptodate",
        "watch": "tsc -w -p ./src",
        "debug": "nodemon --watch ./build --inspect=0.0.0.0:9222 --nolazy ./build/authentication-server.js",
        "cleanup": "npm run prettify-write && npm run linter-write",
        "dev": "nodemon --config \"./util/nodemon.json\"",
        "linter-write": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'",
        "prettify-write": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.js,.json,.css,.scss}\"",
        "start": "ts-node src/authentication-server.ts",
        "test": "echo \"Error: no test specified\" && exit 1",
        "uptodate": "npm update && npm update --dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OpenSlides/openslides-auth-service.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/OpenSlides/openslides-auth-service/issues"
    },
    "homepage": "https://github.com/OpenSlides/openslides-auth-service#readme",
    "devDependencies": {
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.6",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.3",
        "@types/express-jwt": "0.0.42",
        "@types/ioredis": "^4.14.9",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/node": "^13.9.2",
        "@types/pouchdb": "^6.4.0",
        "@types/uuidv4": "^5.0.0",
        "nodemon": "^2.0.2",
        "prettier": "^1.19.1",
        "ts-node": "^8.7.0",
        "tslint": "^6.1.0",
        "typescript": "^3.8.3"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-jwt": "^5.3.1",
        "ioredis": "^4.16.0",
        "jsonwebtoken": "^8.5.1",
        "pouchdb": "^7.2.1",
        "pouchdb-adapter-memory": "^7.2.1",
        "uuid": "^7.0.2",
        "uuidv4": "^6.0.6"
    }
}
